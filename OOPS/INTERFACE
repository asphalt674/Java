//INTERFACE   
public class OOPS
{
    public static void main(String[]args)
    {
       Bear b=new Bear();
       b.eat();
       b.eati();
         System.out.println(b.x);
    }
}
interface  Herbivore
{
    int x=10;
     void eat();
}
interface  Carnivore
{
    void eati();
}
class  Bear implements Carnivore,Herbivore
{
  public void eat()
   {
       System.out.println("Animals eats grass");
   }
   public void eati()
   {
       System.out.println("Animals eats meat");
    }
}
   
